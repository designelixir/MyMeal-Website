<!doctype html>
<html>
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@200;300;400;500;600;700;800&display=swap" rel="stylesheet"> 
    {squarespace-headers}
  </head>
  <body id="{squarespace.page-id}" class="{squarespace.page-classes}">
    <div class="site-container">
      <header class="site-header box-shadow">
        <squarespace:navigation navigationId="siteNav" template="site-navigation" />        
      </header>



      
   
      
      <div class="content-container" role="main" data-content-field="main-content" >
      <div class="radial-gradient">
          <div id="heroBlock" class="flex-start-start flex-column" style="background-image: url({collection.mainImage.assetUrl}); ">
    <div class="heroText-wrapper">
        <squarespace:block-field id="heroText" columns="12"/>
    </div>
    <div class="heroDownload-wrapper">{@|apply downloadInverted.block}</div>
</div>
        {@|apply tabs.block}
</div>
      <main>
          <squarespace:block-field id="pressSection" columns="12"/>
          <div class="flex-center-center">
            {@|apply emailSubscribe.block}
          </div>
          
          <squarespace:block-field id="blogSection" columns="12"/>
          {squarespace.main-content}
          </main>
      </div>
      <footer class="site-footer">
        <squarespace:navigation navigationId="footerNav" template="footer" />        
      </footer>

    

    <!-- combo and minify scripts when not logged in -->
    <squarespace:script src="site.js" combo="{.if authenticatedAccount}false{.or}true{.end}" />

    <!-- other scripts, and footer code injection -->
    {squarespace-footers}
    </div>
  </body>
</html>
