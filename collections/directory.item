					<script>
						function removeAfterSecondComma(str, title) {
							var splitStr = str.split(',');
							if (splitStr.length >= 3) {
								splitStr.splice(2);
							}
							
							var modifiedStr = splitStr.join(',');
							document.getElementById(title + '-address-span-mobile').innerHTML = "&nbsp;üìç " + modifiedStr;
							}
